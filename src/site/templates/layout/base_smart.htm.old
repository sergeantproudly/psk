<!doctype html>
<html lang="ru">
<head>
	<meta charset="utf-8"/>
	<meta name="keywords" content="{{SETTINGS:SEO_KEYWORDS}}">
	<meta property="og:type" content="website">
	<meta property="og:site_name" content="{{SETTINGS:SITE_TITLE}}">
	<meta property="og:title" content="{{PAGE:TITLE}} | {{SETTINGS:SITE_TITLE}}">
	<meta property="og:description" name="description" content="{{SETTINGS:SEO_DESCRIPTION}}">
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=yes"/>
	<link rel="apple-touch-icon" sizes="180x180" href="/images/apple-touch-icon.png">
	<link rel="icon" type="image/png" sizes="32x32" href="/images/favicon-32x32.png">
	<link rel="icon" type="image/png" sizes="16x16" href="/images/favicon-16x16.png">
	<link rel="manifest" href="/images/site.webmanifest">
	<link rel="mask-icon" href="/images/safari-pinned-tab.svg" color="#5bbad5">
	<link rel="shortcut icon" href="favicon.ico">
	<meta name="msapplication-TileColor" content="#da532c">
	<meta name="msapplication-config" content="/public/images/browserconfig.xml">
	<meta name="theme-color" content="#ffffff">
	<title>{{PAGE:TITLE}} | {{SETTINGS:SITE_TITLE}}</title>
    <link type="text/css" rel="stylesheet" href="/assets/css/style.css?1"/>
	<script type="text/javascript" src="/assets/js/jquery-3.1.1.min.js"></script>
	<script type="text/javascript" src="/assets/js/jquery-ui.min.js"></script>
	<script type="text/javascript" src="/assets/js/scripts.js?1"></script>
	<script type="text/javascript" src="/assets/js/jquery.touchSwipe.min.js"></script>
	<script type="text/javascript" src="/assets/js/slick.min.js"></script>
	<script type="text/javascript" src="http://api-maps.yandex.ru/2.1/?lang=ru_RU"></script>
	<!--[if (gte IE 6)&(lte IE 8)]>
	<script type="text/javascript" src="/assets/js/html5.js"></script> 
	<script type="text/javascript">
		var E=('article:aside:audio:canvas:figure:footer:header:hgroup:nav:section:video:output:details:keygen:meter:progress:figcaption:main:mark:summary:time:wbr').split(':');
		for(var c=0;c<E.length;c++){document.createElement(E[c]);} 
	</script>
	<![endif]-->
</head>

<body>
	{{SETTINGS:YANDEX_METRIKA}}
	<div id="layout" class="{{INDEX}}">
		<!-- HEADER --> 
		{{HEADER}}
		<!-- #HEADER -->
		<!-- CONTENT -->
		{{CONTENT}}
		<!-- #CONTENT -->
		<!-- FOOTER -->
		{{INCLUDE:FOOTER}}
		<!-- #FOOTER -->
	</div>
</body>
</html>
